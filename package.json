{
  "name": "testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "lint": "next lint",
    "test": "jest --passWithNoTests --runInBand --no-cache -c jest.config.js",
    "test:unit": "jest --passWithNoTests --runInBand --no-cache -c jest.unit.config.js",
    "test:unit:watch": "jest --passWithNoTests --runInBand --no-cache --watch -c jest.unit.config.js",
    "cypress:open": "start-server-and-test start:dev http://localhost:3000 \"cypress open --e2e\"",
    "cypress:run": "start-server-and-test start:dev http://localhost:3000 \"cypress run --e2e\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.4.0",
    "js-cookie": "^3.0.1",
    "next": "^12.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.16",
    "@types/styled-components": "^5.1.25",
    "cypress": "^12.17.2",
    "express": "^4.18.2",
    "jest": "^28.1.3",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
